{
  "name": "vo2nft-node-backend",
  "version": "0.0.1",
  "description": "VO2NFT node backend service",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "start": "node server",
    "startlocal": "node server",
    "build": "sequelize db:migrate --env production && cd react-ui/ && npm install && npm run build",
    "buildlocal": "sequelize db:migrate --env development && cd react-ui/ && npm install && npm run build"
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "@sendgrid/mail": "^7.6.0",
    "axios": "^0.25.0",
    "body-parser": "^1.19.1",
    "bullmq": "^1.67.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-sslify": "^1.2.0",
    "form-data": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^2.3.0",
    "newrelic": "^8.7.1",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-local-sequelize": "^0.9.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.14.0",
    "sequelize-cli": "^6.4.1",
    "throng": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joereger/vo2nft.git"
  },
